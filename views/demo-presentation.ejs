<div class="reveal">

  <!-- Used to fade in a background when a specific slide state is reached -->
  <div class="state-background"></div>
  
  <!-- Any section element inside of this container is displayed as a slide -->
  <div class="slides">
    
    <!-- SLIDE 1 -->
    <section>
      <h1>Reveal.js</h1>
      <h2>Awesome Javascript Framework for Presentation</h2>
    </section>
    
    <!-- SLIDE 2 -->
    <section>
      <h3>Slide 2</h3>
      <p>You can input all valid html tag here</p>
    </section>
    
    <!-- SLIDE 3 - Vertical Slide -->
    <section>
      <section>
        <h3>Slide 3</h3>
        <p>Slide with vertical slide, press down to see slide below</p>
      </section>
      
      <section>
        <h1>Vertical Slide</h1>
      </section>
    </section>
    
    <section>
      <h1>End Slide</h1>
    </section>
  </div>

</div>





